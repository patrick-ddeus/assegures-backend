{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint .",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests --runInBand",
    "test:watch": "jest --watch --passWithNoTests --runInBand",
    "test:coverage": "cross-env NODE_ENV=test jest --passWithNoTests --runInBand --coverage --collectCoverageFrom='./src/**'",
    "test:migration:run": "npm run test:load-envs prisma migrate deploy",
    "test:migration:generate": "npm run test:load-envs prisma migrate dev",
    "test:load-envs": "dotenv -e .env.test",
    "dev:migration:generate": "npm run dev:load-envs prisma migrate dev",
    "dev:load-envs": "dotenv -e .env.development",
    "dev:seed": "ts-node src/seeders/seed.ts",
    "dev": "npm run dev:load-envs nodemon src/server.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "http-status": "^1.6.2",
    "joi": "^17.9.2",
    "joy": "^0.1.1",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.0",
    "string-strip-html": "^13.4.1",
    "ts-node-dev": "^2.0.0",
    "unidecode": "^0.1.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@types/unidecode": "^0.1.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.6.1",
    "nodemon": "^2.0.22",
    "prettier": "2.8.8",
    "prisma": "^4.16.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
